{
  "name": "express-sql-api",
  "version": "1.0.0",
  "description": "Express API with SQL Database",
  "main": "server.js",
  "homepage": "https://itsahmadnyc.github.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsahmadnyc/express-api-kit.git"
  },
  "author": {
    "name": "Muhammad Ahmad",
    "email": "contact@itsahmadnyc.com",
    "url": "https://itsahmadnyc.github.io/"
  },
  "keywords": [
    "express",
    "api",
    "sql",
    "generator",
    "template",
    "rest",
    "nodejs"
  ],
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "migration:create": "sequelize-cli migration:generate --name",
    "seed:create": "sequelize-cli seed:generate --name",
    "model:create": "sequelize-cli model:generate --name"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.32.1",
    "mysql2": "^3.6.0",
    "pg": "^8.11.0",
    "dotenv": "^16.4.1",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "commander": "^13.1.0",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.5.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.0",
    "pg-hstore": "^2.3.4",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.10.0",
    "multer": "^1.4.4",
    "node-cron": "^3.0.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "joi": "^17.9.2"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
